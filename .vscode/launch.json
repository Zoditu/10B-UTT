    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["--entorno=dev"],
            "program": "${workspaceFolder}\\server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Unit Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "npx",
            "args": ["jest", "escuelas.ui"],
        }
    ]
}